
mainmenu "Benchapp Test Configuration"

config CONFIG_CONFIGURED
    bool
    default y

config CROSS_PREFIX
    string "Cross compile prefix"

choice
    prompt "Target Platform Selection"
    default PLATFORM_LINUX
    help
	linux:
	windows:

config PLATFORM_LINUX
    bool "linux"

config PLATFORM_WINDOWS
    bool "windows"

endchoice

source "Config.md5"
source "Config.aes"
