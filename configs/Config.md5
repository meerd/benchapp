#--------------------------------

config BENCHMARK_MD5_RECIPE3
    bool "Enable test recipe 3"
    default n
    depends on BENCHMARK_MD5

choice
    prompt "Recipe-3 Type"
    depends on BENCHMARK_MD5_RECIPE3
    help
        Time Limited:
